            </geometry>
            <material name="link_material"/>
            <origin rpy="0 0 0" xyz ="0 0 0.075"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0.12" rpy="0 0 0"/>
            <mass value="2" />
            <inertia ixx="0.0061" ixy="0.0" ixz="0.0" iyy="0.0061" iyz="0.0" izz="0.01125"/>
        </inertial>
    </link>

    <joint name="active_joint1" type="revolute">
        <parent link="world"/>
        <child link="link1"/>
