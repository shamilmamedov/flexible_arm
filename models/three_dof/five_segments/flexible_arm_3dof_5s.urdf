    <!-- Wall -->
    <!-- distance from origin to wall = -0.1 -->
    <!-- wall width = 2 -->
    <!-- wall height = 1 -->
    <!-- wall thickness = 0.075 -->
    <link name = "wall_link">
        <visual>
            <origin xyz="0 -0.0375 0.5" rpy="0 0 0" />
            <geometry>
                <box size="2 0.075 1"/>
            </geometry>
            <material name = "wall_material"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.075 2 1"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="15" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>

