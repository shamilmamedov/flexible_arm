<?xml version="1.0"?>
<robot name="flexible_arm">
    <!-- Specify link material -->
    <material name="link_material">
        <!-- <color rgba="0.6 0.6 0.6 1.0"/> -->
        <color rgba="0.059 0.059 0.055 0.851"/>
    </material>

    <material name="goal_material">
        <color rgba="0.87 0.275 0.09 1.0"/>
    </material>

    <material name="wall_material">
        <color rgba="0.6 0.16 0.11 1.0"/>
